<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>DI_DIA_Servlet</servlet-name>
        <servlet-class>PAET_CONTROLLER.DI_DIA_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AR_AREA_Servlet</servlet-name>
        <servlet-class>PAET_CONTROLLER.AR_AREA_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GR_GERENCIA_Servlet</servlet-name>
        <servlet-class>PAET_CONTROLLER.GR_GERENCIA_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DV_DIVISION_Servlet</servlet-name>
        <servlet-class>PAET_CONTROLLER.DV_DIVISION_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DR_DIRECCION_Servlet</servlet-name>
        <servlet-class>PAET_CONTROLLER.DR_DIRECCION_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FC_FACTOR_COMPLEMENTARIO_Servlet</servlet-name>
        <servlet-class>PAET_CONTROLLER.FC_FACTOR_COMPLEMENTARIO_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ET_EQUIPO_TECNOLOGICO_Servlet</servlet-name>
        <servlet-class>PAET_CONTROLLER.ET_EQUIPO_TECNOLOGICO_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PT_PUESTO_Servlet</servlet-name>
        <servlet-class>PAET_CONTROLLER.PT_PUESTO_Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DI_DIA_Servlet</servlet-name>
        <url-pattern>/DI_DIA_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AR_AREA_Servlet</servlet-name>
        <url-pattern>/AR_AREA_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GR_GERENCIA_Servlet</servlet-name>
        <url-pattern>/GR_GERENCIA_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DV_DIVISION_Servlet</servlet-name>
        <url-pattern>/DV_DIVISION_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DR_DIRECCION_Servlet</servlet-name>
        <url-pattern>/DR_DIRECCION_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FC_FACTOR_COMPLEMENTARIO_Servlet</servlet-name>
        <url-pattern>/FC_FACTOR_COMPLEMENTARIO_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ET_EQUIPO_TECNOLOGICO_Servlet</servlet-name>
        <url-pattern>/ET_EQUIPO_TECNOLOGICO_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PT_PUESTO_Servlet</servlet-name>
        <url-pattern>/PT_PUESTO_Servlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
