	<queryString>
		<![CDATA[SELECT COUNT(*),
  PT_DESCRIPCION
FROM PAET_TR_TRABAJADOR
JOIN PAET_PT_PUESTO
  ON PT_PUESTO = PT_CODIGO
INNER JOIN PAET_TT_TELETRABAJADOR
  ON TR_TRABAJADOR = TR_USUARIO
WHERE TR_ESTADO = 'A'
  AND TT_ESTADO = 'A'
GROUP BY PT_DESCRIPCION
ORDER BY 2]]>
	</queryString>
