  TR_APELLIDO2,
  PT_DESCRIPCION,
  TR_FECHA_INGRESO,
  PAET_TT_TELETRABAJADOR.TT_FECHA_INICIO
FROM PAET_TR_TRABAJADOR 
INNER JOIN PAET_TT_TELETRABAJADOR
  ON PAET_TR_TRABAJADOR.TR_USUARIO = PAET_TT_TELETRABAJADOR.TR_TRABAJADOR
JOIN PAET_PT_PUESTO
  ON PT_PUESTO = PT_CODIGO
WHERE TR_ESTADO = 'A'
