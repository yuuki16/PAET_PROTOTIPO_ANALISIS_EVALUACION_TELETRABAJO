	<queryString>
		<![CDATA[SELECT COUNT(*),
  GR_DESCRIPCION
FROM PAET_TR_TRABAJADOR
JOIN PAET_TT_TELETRABAJADOR
  ON TR_USUARIO = TR_TRABAJADOR
JOIN PAET_PT_PUESTO
  ON PT_CODIGO = PT_PUESTO
JOIN PAET_GR_GERENCIA
  ON GR_GERENCIA = GR_CODIGO
WHERE TR_ESTADO = 'A'
  AND TT_ESTADO = 'A'
GROUP BY GR_DESCRIPCION
ORDER BY 2]]>
	</queryString>
